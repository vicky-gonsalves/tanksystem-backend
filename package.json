{
  "name": "tanksystem",
  "version": "0.0.0",
  "main": "src",
  "private": true,
  "scripts": {
    "start": "node .",
    "test": "jest",
    "test:unit": "npm test \"src/**/!(index).test.js\"",
    "test:integration": "npm test \"src/**/index.test.js\"",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "dev": "nodemon -i \"*.test.js\" .",
    "prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
    "lint": "eslint src",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "opn docs/index.html",
    "staging_push_and_deploy": "git push origin HEAD:development",
    "prod_push_and_deploy": "git push origin HEAD:master && git push heroku HEAD:master"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "apidoc-markdown": "^5.1.6",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^26.3.0",
    "cross-env": "^5.2.1",
    "dotenv-safe": "^4.0.3",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest-cli": "^26.4.2",
    "mockgoose": "^8.0.4",
    "nock": "^9.0.2",
    "nodemon": "^1.19.4",
    "opn-cli": "^3.1.0",
    "sinon": "^4.0.1",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bodymen": "^1.1.1",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "express": "^4.17.1",
    "express-force-ssl": "^0.3.2",
    "moment": "^2.27.0",
    "mongoose": "^5.10.5",
    "mongoose-create-unique": "^0.4.4",
    "mongoose-keywords": "^0.3.2",
    "morgan": "^1.10.0",
    "periodic-ping": "^2.0.0",
    "querymen": "^2.1.4",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "web-push": "^3.4.4"
  }
}
